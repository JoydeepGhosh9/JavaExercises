package Lab2ex;

public class removeDuplicates {
	
	static int modifyArray(int n[]) {
		
		for(int i=0;i<n.length;i++) {
			 for(int j=0;j<n.length;j++) {
				 
				 if (n[j]==n[i]) {
					 
				 }
				 
				 if(n[j]>n[i]) {
					 int temp = n[i];
					 n[i] = n[j];
					 n[j] = temp;	 
				 }
			 }
		
	}

	public static void main(String[] args) {
		
		int a[] = {2,3,7,8,2,3,6,7};
		
		int arr[] = modifyArray(a);

	}

}
