package Lab2ex;
import java.util.*;

public class reverseNumandSort {

	public static void main(String[] args) {
		
		int a[] = {5,6,7,4,0,1};
		
		StringBuffer s = new StringBuffer();
		
		for(int i=0;i<a.length;i++) {
			s = s.append(a[i]);
			
			
		}
		s = s.reverse();
		System.out.println(s.charAt(0));
		
		

	}

}
